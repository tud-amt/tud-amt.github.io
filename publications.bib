	author = {ul-Haq, Ehshan and Hondekyn, Marie and Yuksel, Onur and Dransfeld, Clemens and Caglar, Baris},
	editor = {Binetury, Christophe and Jacquemin, Frédéric},
	year = {2024},
	note = {Place: France
Publisher: The European Society for Composite Materials (ESCM) and the Ecole Centrale de Nantes.},
	keywords = {carbon fiber, composites, thin-ply, tow spreading, unidirectional},

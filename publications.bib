@article{brito_ultrasonic_2025,
	title = {Ultrasonic welding of thermoplastic composites: {A} comparison between polyetheretherketone and low-melt polyaryletherketone as resin in the adherends and energy directors},
	volume = {296},
	issn = {13598368},
	shorttitle = {Ultrasonic welding of thermoplastic composites},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1359836825001544},
	doi = {10.1016/j.compositesb.2025.112264},
	abstract = {Our aim with this work was to evaluate how the thermoplastic resin used in the composite adherends and on the energy director affected the static ultrasonic welding process in both parallel and misaligned configurations. Polyetheretherketone (PEEK) and low-melt polyaryletherketone (LMPAEK) were the resins used and their thermomechanical properties were characterized via dynamic-mechanical analysis and modulated differential scanning calorimetry. With parallel adherends, neither the welding time nor the through-thickness heating in the adherends vary significantly. This similarity was attributed to a larger heat capacity of the PEEK energy director counterbalancing its higher viscoelastic heating rate. With misaligned adherends, the welding time was larger for PEEK welds than for LMPAEK welds and LMPAEK adherends presented a larger though-thickness heating. These effects were attributed to the larger bulk viscoelastic heating rate of carbon fibre reinforced/LMPAEK adherends adding up to the lower heat capacity of LMPAEK.},
	language = {en},
	urldate = {2025-05-12},
	journal = {Composites Part B: Engineering},
	author = {Brito, C.B.G. and Teuwen, J. and Dransfeld, C.A. and Villegas, I.F.},
	month = may,
	year = {2025},
	pages = {112264},
	file = {PDF:O\:\\TUD Research\\00_Zotero\\storage\\KUGLZPM7\\Brito et al. - 2025 - Ultrasonic welding of thermoplastic composites A comparison between polyetheretherketone and low-me.pdf:application/pdf},
}

